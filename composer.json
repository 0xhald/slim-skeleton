{
    "require": {
        "slim/slim": "^4.14",
        "nyholm/psr7": "^1.8",
        "nyholm/psr7-server": "^1.1",
        "php-di/php-di": "^7.0",
        "php-di/slim-bridge": "^3.4",
        "cycle/database": "^2.13",
        "symfony/console": "^7.2",
        "tuupola/cors-middleware": "^1.5",
        "monolog/monolog": "^3.9",
        "lcobucci/jwt": "^5.5",
        "lcobucci/clock": "^3.3",
        "symfony/uid": "^7.2",
        "cakephp/chronos": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "scripts":{
        "start": "clear && php -S localhost:3030 -t public",
        "stan": "phpstan analyse --pro src"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1"
    }
}
